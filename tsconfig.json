{
  "compilerOptions": {
    "lib": [
        "es5",
        "es6"
    ],
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noEmit": true,
    "target": "es6",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "allowJs": true,
    "ignoreDeprecations": "5.0",
    "suppressImplicitAnyIndexErrors": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true
  }

}